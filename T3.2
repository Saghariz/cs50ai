#include <iostream>
#include <math.h>
#include <iomanip>

using namespace std;

int main()
{
	int x;
	x <= 1000;
	int n;
	n >= 0;
	int fact = 1;
	double e = 0;
	cin >> x;
	cin >> n;
	for(int i=1; i<=n; i++)
	{
		if(i>1)
		fact = fact * (i-1);
		
		pow(e , x) = pow(e , x-1) + (pow(x , n-1) / fact);
	}
	cout << fixed << setprecision(3);
	cout << e;
	
	return 0;
}
